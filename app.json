{
  "expo": {
    "name": "My GPT",
    "slug": "my-gpt",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000,
      "url": "https://u.expo.dev/88f3c3ca-dc19-43ed-9ff3-7b3821e45b75"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.votepurchase.mygpt",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "チャットに投稿するためにマイクを使用します",
        "NSCameraUsageDescription": "チャットに画像を投稿するためにカメラを使用します",
        "NSPhotoLibraryUsageDescription": "チャットに画像を投稿するためにメディアライブラリを使用します",
        "CFBundleDevelopmentRegion": "ja_JP"
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "net.votepurchase.mygpt",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "permissions": [
        "VIBRATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "88f3c3ca-dc19-43ed-9ff3-7b3821e45b75"
      }
    }
  }
}
